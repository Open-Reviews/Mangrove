{
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "printWidth": 100,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": false,
  "parser": "babel",
  "proseWrap": "preserve",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "parser": "css"
      }
    }
  ]
}